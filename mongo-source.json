{
    "name": "mongo-source-connector",
    "config": {
      "connector.class": "com.mongodb.kafka.connect.MongoSourceConnector",
      "tasks.max": "1",
      "connection.uri": "mongodb://mongodb:27017",
      "database": "test-db",
      "collection": "testCollection",
      "topic.prefix": "mongo.",
      "poll.max.batch.size": "1000",
      "poll.await.time.ms": "5000"
    }
  }
  